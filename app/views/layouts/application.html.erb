<!DOCTYPE html>
<html>
<head>
  <title>WeFreshed</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
  <%= include_gon %>

  <meta name="viewport" content="width=device-width, initial-scale=1.0">

  <link href='https://fonts.googleapis.com/css?family=Questrial|Montserrat' rel='stylesheet' type='text/css'>

<!--    <script src="http://code.jquery.com/ui/1.10.3/jquery-ui.js"></script> -->


</head>

<body id='bg'>


<%= render "layouts/user_header" %>


  
<!-- FLASH MESSAGE -->

    <div class="alert-container">
      <% flash.each do |name, message| %>
        <div class="alert alert-<%= name == "alert" || name == "notice" ? "warning" : "name" %> alert-dismissible" role="alert">
          <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        <%= message %>
          </div>
      <% end %>
    </div>

 
    <%= yield %>


    
  </div>



</body>
</html>
